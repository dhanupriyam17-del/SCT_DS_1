from google.colab import files
uploaded = files.upload()
import pandas as pd
import matplotlib.pyplot as plt
df = pd.read_excel('populatino 2.xlsx')
df.head()
print('Available columns:')
print(df.columns.tolist())

# Plotting the distribution of 'PROVINCE' as an example
province_counts = df['PROVINCE'].value_counts()

plt.figure(figsize=(10, 6))
plt.bar(province_counts.index, province_counts.values)
plt.xlabel('PROVINCE')
plt.ylabel('Number of Divisions/Districts')
plt.title('Distribution of Provinces')
plt.xticks(rotation=45, ha='right')
plt.tight_layout()
plt.show()

